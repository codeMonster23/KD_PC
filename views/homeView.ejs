        <script>
            $('.header .dropdown').hover(function() {
                $(this).find('.dropdown-menu').stop().slideToggle();
            });
            $('.header .hr .search-icon').click(function(e) {
                e.stopPropagation();
                $('.search').stop().slideDown();
            });
            $('.search').click(function(e) {
                e.stopPropagation();
            });
            $('.search').mouseleave(function() {
                $(this).stop().slideUp();
            });
            // 搜索类型切换
            var type = 0;
            $('.search-tab span').click(function() {
                type = $(this).index();
                $(this).addClass('selected').siblings().removeClass('selected');
            });
            //热门榜单切换
            $('.s2-hot').on('mouseenter', '.s2-hot-item', function() {
                    $('.s2-hot-item').removeClass('active');
                    $(this).toggleClass('active');
                }

            ); //作品类型切换
            var tabA = $('.s1-tab span');
            var s1Icon = $('.s1-item-img i');
            var index = 0;
            tabA.click(function(e) {
                    e.stopPropagation();
                    index = $(this).index();
                    $(this).addClass('selected').siblings().removeClass('selected');
                    // if (index > 0) {
                    //     s1Icon.show().attr('class', '').addClass('s1-item-icon' + index);
                    // }
                    $('.s1-items').eq(index).show().siblings('.s1-items').hide();
                }

            );

            var login = $('#login');
            login.click(function() {
                location.href = '/temp/userCenter/login?v=' + Date.now();
            })

            var searchBtn = $('.search-btn');
            var enterControl = searchBtn.attr('data-enter-control');
            searchBtn.on('click', function() {
                    var keyword = $(this).siblings('input').val();
                    if (keyword.trim().length == 0) {
                        return;
                    }
                    var typeArr = ['home', 'works', 'microbook', 'kdh', 'period', 'book'];
                    if (type == 6) {
                        location.href = encodeURI('/literature/literatureResult?fulltext=' + keyword);
                    } else {
                        location.href = encodeURI('/search/' + typeArr[type] + '?keyword=' + keyword);
                    }

                })
                // 回车 提交 enterControl可以区分其他回车需求
            if (enterControl == 1) {
                $(document).bind("keydown", function(e) {
                    if (e.keyCode == 13) {
                        searchBtn.click();
                    }
                })
            }
            $(window).scroll(function() {
                var A = $(window).scrollTop();
                if (A > 0) {
                    $('.header').addClass('fixed');
                } else {
                    $('.header').removeClass('fixed');
                }
            });
        </script>
